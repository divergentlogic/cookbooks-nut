# Generated by Chef for <%= node[:fqdn] %>

<% unless node['nut']['max_age'].nil? %>
MAXAGE = <%= node['nut']['max_age'] %>
<% end %>

<% unless node['nut']['state_path'].nil? %>
STATEPATH = <%= node['nut']['state_path'] %>
<% end %>

<% unless node['nut']['listen'].nil? %>
<% node['nut']['listen'].each do |address| %>
<%= "LISTEN #{address} 3493" %>
<% end %>
<% end %>

<% unless node['nut']['max_connections'].nil? %>
MAXCONN = <%= node['nut']['max_connections'] %>
<% end %>

<% unless node['nut']['certificate_file'].nil? %>
CERTFILE = <%= node['nut']['certificate_file'] %>
<% end %>
